@model P3Image_Teste.Administracao.Models.CamposModel

@{
    ViewBag.Title = "Excluir";
}

<h2>Excluir</h2>

<h3>Você deseja excluir este registro?</h3>
<fieldset>
    <legend>Campos</legend>
     <div class="display-label">
         @Html.DisplayName("Subcategoria")
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.subCategoria.descricao)
    </div>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.ordenacao)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ordenacao)
    </div>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.descricao)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.descricao)
    </div>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.tipo)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.tipoTexto)
    </div>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.listacheckDrop)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.listacheckDrop)
    </div>
</fieldset>
@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.HiddenFor(X => X.subCategoriaId)
    <p>
        <input type="submit" value="Excluir" /> |
        @Html.ActionLink("Voltar para listagem", "Index", new { Id=Model.subCategoriaId })

    </p>
}
